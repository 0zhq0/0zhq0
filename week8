#include<iostream>
#include<vector>
using namespace std;

void print_array10(ostream& os, int* a)
{
    for(int i=0;i<10;i++)
    {
        os<<a[i]<<" ";
    }
}

void print_array(ostream& os, int* a, int n)
{
    for(int i=0;i<n;i++)
    {
        os<<a[i]<<" ";
    }
}

void print_vector(ostream& os, vector<int>& v)
{
    for(int i=0;i<v.size();i++)
    {
        os<<v[i]<<" ";
    }
}

int main()
{
    int* array1=new int[10];
    cout<<"array1:";
    for(int i=0;i<10;i++)
    {
        cout<<array1[i]<<" ";
    }
    delete[] array1;

    int* array2=new int[10];
    {
        for(int i=0;i<10;i++)
        {
            array2[i]=100+i;
            cout<<"array2["<<i<<"]="<<array2[i];
        }
    }
    delete[] array2;

    int* array3=new int[11];
    {
        for(int i=0;i<11;i++)
        {
            array3[i]=100+i;
            cout<<"array3["<<i<<"]="<<array3[i];
        }
    } 
    delete[] array3;

    int* array4=new int[20];
    {
        for(int i=0;i<20;i++)
        {
            array4[i]=100+i;
            cout<<"array4["<<i<<"]="<<array4[i];
        }
    } 
    delete[] array4;

    int a=7;
    int* p1=&a;
    cout<<"p1="<<&p1;
    cout<<"*p1="<<*p1;
    int* p2=new [7];
    for(int i=0;i<7;i++)
    {
        p2[i]=2^i;
    }
    int* p3;
    p3=p2;
    p2=p1;
    cout<<"p1:"<<*p1;
    cout<<"p2:"<<*p2;
    delete p2;

    int* array5=new int[10];
    {
        for(int i=0;i<7;i++)
        {
            array5[i]=2^i;
        }
    }
    p2=array5;
    p1=p2;
    vector<int> vec1;
    for (int i = 0; i<10; ++i)
        vec1.push_back(2^i);
    vector<int> vec2;
    for (int i = 0; i<vec1.size(); ++i)
        vec2.push_back(vec1[i]);
    print_vector(cout,vec2);
}
